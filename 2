#include <iostream>

using namespace std;

int main()
{
int n,i,v;
cout<<"N= "; cin>>n;
int *mas=new int [n];
for (i=0;i<n;i++) {cin>>mas[i];}
i=0; v=1;
while (i<n-2) 
{
if (mas[i+1]-mas[i]==mas[i+2]-mas[i+1]) 
{
v=v+1; 
}
i++;
} 
if (v==n-1){cout<<"d= "<<mas[1]-mas[0]<<endl;}
else {cout<<"0"<<endl;}
return 0;
} 
