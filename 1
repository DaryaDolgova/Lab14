#include <iostream>

using namespace std;

int main()
{
int n,K,l,t,s,i; double z;
cout<<"N= "; cin>>n;
cout<<"K= "; cin>>K;
cout<<"L= "; cin>>l;
int *mas=new int [n];
for (i=0;i<n;i++) {cin>>mas[i];}
s=0; t=0;
for (i=0;i<n;i++)
{
if (i>=K&&i<=l)
{
s=s+mas[i];
t=t+1;
}
}
z=s/t;
cout<<z<<endl;
return 0;
}
